#Python 
#2.1. 
def gcd(a, b):
    if b == 0:
        return a
    return gcd(b, a % b)

numbers = [(48, 18), (56, 42), (17, 13), (100, 25), (0, 5)]

print("Результаты работы функции:")
for a, b in numbers:
    result = gcd(a, b)
    print(f"НОД({a}, {b}) = {result}")

#Результаты работы функции:
#НОД(48, 18) = 6
#НОД(56, 42) = 14
#НОД(17, 13) = 1
#НОД(100, 25) = 25
#НОД(0, 5) = 5
