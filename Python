#Список
chars = list('Hello')
print(chars)

#Стек
my_stack = []
my_stack.append(10)
my_stack.append(20)
my_stack.append(30)
top_element = my_stack[-1]
print(f"Верхний элемент: {top_element}")
removed_element = my_stack.pop()
print(f"Удаленный элемент: {removed_element}")
print(f"Стек после удаления: {my_stack}")
if not my_stack:
    print("Стек пуст")
else:
    print(f"Стек не пуст, последний элемент: {my_stack[-1]}")
